    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="Да"/>
      <xs:enumeration value="Нет"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tUnspecifiedVolumePurchaseType">
    <xs:annotation>
      <xs:documentation/>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="unitPrice"/>
      <xs:enumeration value="reductionPercentage"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tDocument">
    <xs:annotation>
      <xs:documentation>Тип: Вид документа</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string"/>
  </xs:simpleType>
  <xs:simpleType name="tSum">
    <xs:annotation>
      <xs:documentation>Тип: Сумма</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="21"/>
      <xs:pattern value="(-)?\d+(\.\d{1,2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tPrepayType">
    <xs:annotation>
      <xs:documentation>Тип: Размер аванса</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="6"/>
      <xs:pattern value="(\d{1,2}(\.\d{1,2})?)|100(.0{1,2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tSumPositive">
    <xs:annotation>
      <xs:documentation>Тип: Сумма без отрицаиельных значений</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="21"/>
      <xs:pattern value="(\d)+(\.\d{1,2})?"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tPrice">
    <xs:annotation>
      <xs:documentation>Тип: Цена</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="21"/>
      <xs:pattern value="([1-9](\d)*(\.\d{1,2})?)|(0\.[1-9])|(0\.\d[1-9])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tPrice4">
    <xs:annotation>
      <xs:documentation>Тип: Цена (4 знака после запятой)</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:maxLength value="21"/>
      <xs:pattern value="([1-9](\d)*(\.\d{1,4})?)|(0\.\d{0,3}[1-9])"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tOfferNtin">
    <xs:annotation>
      <xs:documentation>Тип: NTIN(EAN) </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="13"/>
      <xs:maxLength value="16"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="tShortName">
    <xs:annotation>
      <xs:documentation>Тип: Короткая строка</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
